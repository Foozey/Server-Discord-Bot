{
	"schemaVersion": 1,
	"id": "server-discord-bot",
	"version": "${version}",
	"name": "Server Discord Bot",
	"description": "A Minecraft mod that links your server with a discord bot.",
	"authors": [
		"Fooze"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/server-discord-bot",
        "issues": "https://github.com/foozey/server-discord-bot/issues",
		"sources": "https://github.com/foozey/server-discord-bot"
	},
	"license": "MIT",
	"icon": "assets/server-discord-bot/icon.png",
	"environment": "server",
	"entrypoints": {
		"server": [
			{
				"value": "com.fooze.serverdiscordbot.ServerDiscordBot",
				"adapter": "kotlin"
			}
		]
	},
	"depends": {
		"fabricloader": ">=0.17.3",
		"minecraft": "~1.21.10",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	}
}